# tetris project
tetris-ai() {
	local size="$1"
	local aps="$2"

    	if [ "$size" == "--help" ]; then
		echo "tetris-ai -screen_size -actions_per_second"
    	else
		home 
		vsc 
        	cd Python/Tetris
    		enva
		pip install -r requirements.txt
    		p3 test_model.py "$size" "$aps"	
    	fi
}

tetris() {
	home
	vsc
	cd Python/Tetris
	enva
	pip install -r requirements.txt
	p3 manual_play.py
}

